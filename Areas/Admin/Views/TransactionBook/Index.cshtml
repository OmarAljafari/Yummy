@model IEnumerable<Yummy.Models.TransactionBook>

@{
    ViewData["Title"] = "Index";
}

<div class="full_container">
    <div class="container">
        <div class="full_height">
            <div class="col-md-12 mb-3">
                <div class="white_shd full margin_bottom_30">
                    <div class="full graph_head">
                        <div class="heading1 margin_0">
                        </div>
                    </div>
                    <div class="table_section padding_infor_info">
                        <div class="table-responsive-sm">
                            <table class="table table-dark table-striped">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookDate)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookEmail)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookMessage)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookName)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookNumberOfPeople)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookPhone)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionBookTime)
                                        </th>
                                       

                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookDate)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookEmail)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookMessage)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookNumberOfPeople)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookPhone)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionBookTime)
                                            </td>



                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
