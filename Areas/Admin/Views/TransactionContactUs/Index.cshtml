@model IEnumerable<Yummy.Models.TransactionContactUs>

@{
    ViewData["Title"] = "Index";
}

<div class="full_container">
    <div class="container">
        <div class="full_height">
            <div class="col-md-12 mb-3">
                <div class="white_shd full margin_bottom_30">
                    <div class="full graph_head">
                        <div class="heading1 margin_0">
                          
                        </div>
                    </div>
                    <div class="table_section padding_infor_info">
                        <div class="table-responsive-sm">
                            <table class="table table-dark table-striped">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionContactUsEmail)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionContactUsMessage)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionContactUsName)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.TransactionContactUsSubject)
                                        </th>
                                       
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionContactUsEmail)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionContactUsMessage)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionContactUsName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.TransactionContactUsSubject)
                                            </td>



                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


